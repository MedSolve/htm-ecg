% Read source image and return a bitmap of that
function bitmap = imageMap(src) 

    % get data from src
    data = imread(src);
    
    % get size bounderies
    [yM, x] = size(data);
    
    % create reference to bitmap
    bitmap = zeros(1, );
    
    %create a vector by loop the dataset
    for i = 1:yM
        bitmap = [bitmap data(i,:)];
    end
end